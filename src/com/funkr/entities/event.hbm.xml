<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
	<class name="com.funkr.entities.Event" table="event" catalog="funkr">
		<id name="Event_id" type="string">
			<column name="event_id" />
		</id>
		<one-to-many name="Artists" class="com.funkr.entities.Artists" 
			cascade="save-update">
			
			</one-to-many>
		<property name="Event_url" type="string">
			<column name="event_url" length="64" not-null="false" unique="false" />
		</property>
		<property name="Ticket_url" type="string">
			<column name="ticket_url" length="64" not-null="false" unique="false" />
			</property>
		<one-to-one name="Venue" class="com.funkr.entities.Venue"
			cascade="save-update">
			</one-to-one>
	</class>
</hibernate-mapping>