<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
	<class name="com.funkr.entities.Event" table="event" catalog="funkr">
		<id name="eventid" type="java.lang.Integer">
			<column name="event_id" />
			<generator class="identity" />
		</id>
		<property name="event_url" type="string">
			<column name="event_url" length="64" not-null="false" unique="false" />
		</property>
		<property name="ticket_url" type="string">
			<column name="ticket_url" length="64" not-null="false" unique="false" />
		</property>
		<one-to-one name="venue" class="com.funkr.entities.Venue"
			cascade="save-update"></one-to-one>
	</class>
</hibernate-mapping>